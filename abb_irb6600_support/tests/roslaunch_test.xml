<launch>
  <arg name="ip_str" value="127.0.0.1" />

  <group ns="load_irb6640__">
    <include file="$(find abb_irb6600_support)/launch/load_irb6640.launch"/>
  </group>

  <group ns="test_irb6640__">
    <include file="$(find abb_irb6600_support)/launch/test_irb6640.launch"/>
  </group>

  <group ns="robot_interface_download_irb6640__">
    <include file="$(find abb_irb6600_support)/launch/robot_interface_download_irb6640.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
    </include>
  </group>

  <group ns="robot_state_visualize_irb6640__">
    <include file="$(find abb_irb6600_support)/launch/robot_state_visualize_irb6640.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
    </include>
  </group>
</launch>
