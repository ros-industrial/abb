<!--
Manipulator specific version of abb_driver's 'robot_interface.launch'.
Defaults provided for IRB 4400:
- J23_coupled = true
Usage:
robot_interface_download_irb4400.launch robot_ip:=<value>
-->
<launch>
    <arg name="robot_ip" />
    <arg name="J23_coupled" default="true" />
    <rosparam command="load" file="$(find abb_irb4400_support)/config/joint_names_irb4400.yaml" />
    
    <include file="$(find abb_common)/launch/robot_interface.launch">
        <arg name="robot_ip" value="$(arg robot_ip)" />
        <arg name="J23_coupled" value="$(arg J23_coupled)" />
    </include>
</launch>
