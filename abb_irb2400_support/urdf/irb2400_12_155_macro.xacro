        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
           <mesh filename="package://abb_irb2400_support/meshes/irb2400_common/visual/link_1.dae"/>
        </geometry>
        <xacro:material_abb_orange />
      </visual>
      <collision>
