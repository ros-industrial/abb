      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://abb_irb2600_support/meshes/visual/base_link.STL" />
        </geometry>
        <material name="">
          <color rgba="1 0.50196 0 1" />
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://abb_irb2600_support/meshes/collision/base_link.STL" />
        </geometry>
      </collision>
    </link>
